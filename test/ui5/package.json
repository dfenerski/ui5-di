{
    "name": "com.github.dfenerski.infinite_velocity",
    "version": "1.0.0",
    "description": "UI5 Application: com.github.dfenerski.infinite_velocity",
    "author": "Dimitar Fenerski",
    "license": "Apache-2.0",
    "scripts": {
        "clean": "rimraf dist coverage",
        "build:ts": "babel webapp --out-dir dist --extensions \".ts,.js\" --copy-files",
        "build:ui5": "ui5 build --exclude-task=minify --clean-dest --all",
        "build:dev": "ui5 build --exclude-task=* --include-task=ui5-tooling-transpile-task",
        "build:clean": "ui5 build --clean-dest",
        "build:opt": "ui5 build self-contained --clean-dest --all",
        "start": "ui5 serve --port 8080 -o index.html",
        "start-cdn": "ui5 serve --port 8080 -o index-cdn.html",
        "start:dist": "npm start -- --config ui5-dist.yaml",
        "start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
        "ts-typecheck": "tsc --noEmit",
        "lint": "eslint webapp",
        "karma": "karma start",
        "karma-ci": "karma start karma-ci.conf.js",
        "karma-ci-cov": "karma start karma-ci-cov.conf.js",
        "test": "npm run lint && npm run karma-ci-cov",
        "wdi5": "wdio run ./webapp/test/e2e/\\wdio.conf.ts"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.9",
        "@babel/core": "^7.23.9",
        "@babel/plugin-proposal-decorators": "^7.23.9",
        "@babel/preset-typescript": "^7.23.3",
        "@openui5/types": "^1.120.6",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "@ui5/cli": "^3.9.0",
        "@wdio/cli": "^8.29.7",
        "@wdio/local-runner": "^8.29.7",
        "@wdio/mocha-framework": "^8.29.3",
        "@wdio/spec-reporter": "^8.29.7",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "eslint": "^8.56.0",
        "karma": "^6.4.2",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage": "^2.2.1",
        "karma-ui5": "^3.0.4",
        "karma-ui5-transpile": "^3.3.3",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "ui5-middleware-livereload": "^3.0.2",
        "ui5-tooling-transpile": "^3.3.3",
        "wdio-ui5-service": "^2.0.6"
    },
    "dependencies": {
        "reflect-metadata": "^0.2.1"
    }
}
