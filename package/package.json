{
    "name": "ui5-di",
    "version": "0.0.11",
    "description": "Dependency Injection library for SAPUI5 / OpenUI5",
    "author": "Dimitar Fenerski",
    "license": "Apache-2.0",
    "keywords": [
        "UI5",
        "DI",
        "Dependency Injection"
    ],
    "types": "dist/index.d.ts",
    "scripts": {
        "clean": "rimraf dist coverage",
        "build": "ui5 build --clean-dest",
        "build:ts-interfaces": "npx @ui5/ts-interface-generator",
        "start": "ui5 serve -o test-resources/my/bookshop/lib/Example.html",
        "start:dist": "npm start -- --config ui5-dist.yaml",
        "testsuite": "ui5 serve -o test-resources/my/bookshop/lib/qunit/testsuite.qunit.html",
        "ts-typecheck": "tsc --noEmit",
        "lint": "eslint src test",
        "karma": "karma start",
        "karma-ci": "karma start karma-ci.conf.js",
        "karma-ci-cov": "karma start karma-ci-cov.conf.js",
        "test": "npm run lint && npm run karma-ci-cov",
        "test:bare": "node --no-warnings=ExperimentalWarning --loader ts-node/esm",
        "prepublish": "npm run build && cp ../README.md ."
    },
    "devDependencies": {
        "@openui5/types": "1.120.7",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "@ui5/cli": "^3.9.0",
        "@ui5/ts-interface-generator": "^0.8.1",
        "eslint": "^8.56.0",
        "karma": "^6.4.2",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage": "^2.2.1",
        "karma-ui5": "^3.0.4",
        "karma-ui5-transpile": "^3.3.3",
        "npm-run-all": "^4.1.5",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "ui5-middleware-livereload": "^3.0.2",
        "ui5-tooling-transpile": "^3.3.3"
    },
    "dependencies": {
        "@babel/plugin-proposal-decorators": "^7.23.9",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "reflect-metadata": "^0.2.1"
    }
}
